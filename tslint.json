{
  "extends": "tslint:latest",
  "rules": {
    "curly": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "jsx-double"
    ],
    "trailing-comma": [
      false
      ],
    "no-console":[
      false
      ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ]
  },
  "jsRules": {
    "curly": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ]
  },
  "rulesDirectory": [
  ]
}
